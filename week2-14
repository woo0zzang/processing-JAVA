//Translated Characrer

void setup() {
  size(480, 270);  
}
void draw() {
  background(255);  
  ellipseMode(CENTER);
  rectMode(CENTER); 
  translate(mouseX, mouseY); /* Draw everything relative to (mouseX, mouseY)*/
  stroke(0);
  fill(175);
  rect(0, 0, 20, 100);
  stroke(0);
  fill(255);
  ellipse(0, -30, 60, 60); 
  stroke(0);
  fill(0); 
  ellipse(-19, -30, 16, 32); 
  ellipse(19, -30, 16, 32); 
  stroke(0);
  line(-10, 50, -20, 60);
  line(10, 50, 20, 60);
}
